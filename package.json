{
  "name": "Flor",
  "version": "3.0.0",
  "description": "Build config for lit-element with Babel and Rollup",
  "main": "index.js",
  "repository": "",
  "scripts": {
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "copyindex": "cp src/index.html build/components",
    "copywc": "cp -r build/*.js build/components && cp -r node_modules/@webcomponents/webcomponentsjs/bundles build/components && cp node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js build/components",
    "copydep": "cp -r node_modules/moment build/components",
    "build": "tsc && rm -rf build/components && mkdir build/components && npm run copyindex && npm run copydep && rollup -c",
    "start": "serve build"
  },
  "author": "Tecnova",
  "license": "",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "serve": "^10.1.1",
    "typescript": "^3.8.3",
    "web-component-analyzer": "^1.0.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@doubletrade/lit-datatable": "^0.4.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@lit-element-bootstrap/button": "^1.0.0",
    "@lit-element-bootstrap/content": "^1.0.0-alpha.3",
    "@vaadin/vaadin": "^15.0.6",
    "@vaadin/vaadin-button": "^2.3.0",
    "@vaadin/vaadin-date-picker": "^4.2.0",
    "@vaadin/vaadin-form-layout": "^2.2.0",
    "@vaadin/vaadin-grid": "^5.6.6",
    "@vaadin/vaadin-list-box": "^1.3.0",
    "@vaadin/vaadin-ordered-layout": "^1.3.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.5.0",
    "custom-elements-es5-adapter": "^1.0.0",
    "fa-icons": "^0.2.0",
    "lit-element": "^2.0.0-rc.2",
    "lit-element-datepicker": "0.0.3",
    "lit-fontawesome": "^0.1.3",
    "moment": "^2.27.0",
    "moment-es6": "^1.0.0",
    "moment-with-locales-es6": "^1.0.1",
    "sugar": "^2.0.6"
  }
}
